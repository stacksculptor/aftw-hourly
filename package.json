  "devDependencies": {
    "nodemon": "^1.11.0",
    "prettier": "^0.13.1"
  },
  "scripts": {
    "clean": "rm -rf build/*",
    "dev": "sharetribe-scripts start",
    "build": "sharetribe-scripts build",
